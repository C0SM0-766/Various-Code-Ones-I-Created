  campaignTrail_temp.credits = "<button onclick='credits()'>C0SM0</button>";
  
  credits = function() {
    credits = ["CREDITS", "C0SM0 - Writing & Coding", "NeoIntenew - Original mod idea, Writing, and various images", "DecStarG - Many different codes"]
    text = "CREDITS:\n\n"
    for (i in credits) {
        text += credits[i] + "\n"
    }
